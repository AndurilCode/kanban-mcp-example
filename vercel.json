{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "framework": null,
  "installCommand": "npm install",
  "buildCommand": "npm run build",
  "outputDirectory": "public",
  "functions": {
    "api/index.ts": {
      "includeFiles": "public/index.html"
    }
  },
  "rewrites": [{ "source": "/(.*)", "destination": "/api/index" }]
}
